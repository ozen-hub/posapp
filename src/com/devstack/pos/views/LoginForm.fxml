<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="270.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devstack.pos.controller.LoginFormController">
   <children>
      <Label layoutX="143.0" layoutY="28.0" text="Log in">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" layoutX="12.0" layoutY="98.0" prefHeight="34.0" prefWidth="313.0" promptText="email">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="12.0" layoutY="144.0" prefHeight="34.0" prefWidth="313.0" promptText="password">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </PasswordField>
      <Button layoutX="213.0" layoutY="187.0" mnemonicParsing="false" onAction="#signInOnAction" prefHeight="34.0" prefWidth="112.0" text="sign in" />
      <Hyperlink layoutX="99.0" layoutY="233.0" onAction="#navigateToSignUp" text="Don't have an account?" underline="true" />
   </children>
</AnchorPane>
